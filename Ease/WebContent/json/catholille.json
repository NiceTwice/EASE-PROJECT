
{
    "website":"la catho lille",
    
    "urls":{
        "login":"https://sso.univ-catholille.fr/cas/login"
    },
    
    "elements":{
        
        "userField":{
            "id":"username"
        },
        "passField":{
            "id":"password"
        },
        "buttonLogIn":{
            "tagName":"input",
            "className":"btn-submit",
            "name":"submit",
            "type":"submit"
        }
        
    },
    
    "connectionSteps":[
        {
            "action":"goTo",
            "url":"login"
        },
        
        {
                "action":"catchFail",
                "if":"missingElement",
                "elementName":"passField",
                "actionsIfFail":[],
                "send":"alreadyConnected"
        },
        
        {
            "action":"do",
            "elements":[
                {
                    "name":"userField",
                    "action":"fill",
                    "value":"user"
                },
                {
                    "name":"passField",
                    "action":"fill",
                    "value":"pass"
                },
                {
                    "name":"buttonLogIn",
                    "action":"click"
                }
            ]
        },
        
        {
            "action":"nextPage",
            "lastPage":true
        },
        
        {
            "action":"catchFail",
            "if":"existingElement",
            "elementName":"passField",
            "actionsIfFail":[],
            "send":"wrongPassword"
        }
    ]
}
