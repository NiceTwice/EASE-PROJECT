{
    "website":"unify ieseg",
    
    "urls":{
        "login":"https://unify.ieseg.fr"
    },
    
    "elements":{
        
        "userField":{
            "id":"txtUserName"
        },
        "passField":{
            "id":"txtPassword"
        },
        "buttonLogIn":{
            "id":"SubmitButton"
        }
    },
    
    "connectionSteps":[
        {
            "action":"goTo",
            "url":"login"
        },
        
        {
            "action":"do",
            "elements":[
                {
                    "name":"userField",
                    "action":"fill",
                    "value":"user"
                },
                {
                    "name":"passField",
                    "action":"fill",
                    "value":"pass"
                },
                {
                    "name":"buttonLogIn",
                    "action":"click"
                }
            ]
        },
        
        {
            "action":"nextPage",
            "lastPage":true
        },
        
        {
            "action":"catchFail",
            "if":"existingElement",
            "elementName":"passField",
            "actionsIfFail":[],
            "send":"wrongPassword"
        }
    ]
}

