{
   "background": {
      "page": "background.html",
      "persistent": false
   },
   "browser_action": {
      "default_icon": "images/logo_296.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "logout-ease": {
         "description": "Logout from ease and all connected websites.",
         "suggested_key": {
            "default": "Ctrl+L",
            "mac": "Ctrl+L"
         }
      },
      "open-ease": {
         "description": "Open Ease in a new tab.",
         "suggested_key": {
            "default": "Ctrl+Space",
            "mac": "Ctrl+Space"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "jquery-3.1.0.js", "extension.js", "easeListener.js" ],
      "matches": [ "https://ease.space/*", "https://www.ease.space/*", "http://ease.space/*", "http://www.ease.space/*", "http://localhost:8080/HelloWorld/*", "https://localhost:8443/HelloWorld/*" ]
   } ],
   "description": "Ease extension, log you in and out",
   "icons": {
      "128": "images/logo_128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Ease",
   "permissions": [ "tabs", "\u003Call_urls>", "http://*/*", "https://*/*", "webNavigation", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.20"
}
