{
"manifest_version": 2,
    
"name" :"Plugin test",
"version" :"0.0.1",
"description" :"pour faire des tests",

      
"content_scripts": [
    {
      "matches": ["https://passwords.google.com/*"],
      "js": ["jquery-3.1.0.js", "scrapPsswdChrome.js"],
       "css":["facebookOverlay.css"]
    },
    {
      "matches": ["https://www.facebook.com/settings?tab=applications"],
      "js": ["jquery-3.1.0.js", "scrapPsswdFacebook.js"],
       "css":["facebookOverlay.css"]
    },
    {
      "matches": ["https://www.linkedin.com/psettings/third-party-applications"],
      "js": ["jquery-3.1.0.js", "scrapPsswdLinkedin.js"],
       "css":["facebookOverlay.css"]
    }
  ],
    
"background":{
    "scripts" : ["background.js"],
    "persistent": false
},
    
"permissions": ["tabs","\u003Call_urls\u003E","http://*/*", "https://*/*","webNavigation","storage"]
}